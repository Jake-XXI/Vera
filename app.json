{
  "expo": {
    "name": "Vera",
    "slug": "vera",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#635BFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jacobbecker.vera",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Vera uses your location to show nearby listings and connect you with local sellers.",
        "NSLocationAlwaysUsageDescription": "Vera uses background location to improve nearby recommendations (optional).",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.jacobbecker.vera",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#635BFF"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "edgeToEdgeEnabled": true
    },
"plugins": [
  [
    "expo-build-properties",
    {
      "ios": {
        "useFrameworks": "static",
        "deploymentTarget": "15.1"
      }
    }
  ],
  [
    "expo-camera",
    {
      "cameraPermission": "Vera needs camera access to take your verification selfie."
    }
  ],
  [
    "expo-location",
    {
      "locationAlwaysAndWhenInUsePermission": "Vera uses your location to show nearby verified deals and local sellers.",
      "isIosBackgroundLocationEnabled": false
    }
  ],
  [
    "@stripe/stripe-react-native",
    {
      "merchantIdentifier": "merchant.com.jacobbecker.vera",
      "enableGooglePay": true
    }
  ]
],

    "extra": {
      "eas": {
        "projectId": "c3b28a19-3e81-4080-bcf9-ae5a50806b02"
      }
    },
    "owner": "jacobbecker1212"
  }
}
